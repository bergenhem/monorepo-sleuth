{
  "name": "monorepo-sleuth",
  "packageManager": "yarn@1.22.17",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "start": "yarn --cwd apps/frontend start",
    "next": "yarn --cwd apps/nextapp dev -p 4000",
    "dev": "npm-run-all --parallel start next"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
